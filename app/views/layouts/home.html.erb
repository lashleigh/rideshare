<!DOCTYPE html>
<html>
<head>
  <title>Rideshare</title>
  <%= stylesheet_link_tag :all %>
  <link rel="stylesheet/less" type="text/css" href="/stylesheets/application.less">

  <%= javascript_include_tag 'jquery-1.4.4.min', 'jquery.rails', 'less-1.1.3.min' %>
  <%= javascript_include_tag 'application', "jquery-ui-1.8.7.custom.min" %>
  <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>

  <%= csrf_meta_tag %>
</head>
<body>
  <div id="wrap">
    <div id="content">
      <%= render 'home/header' %>
      <section id="index">
      <%= yield %>
      </section>
      <%= render 'home/footer' %>
    </div>
  </div>
</body>
</html>
